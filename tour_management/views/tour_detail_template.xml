        <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
        <!--Tour detail  template-->
        <template id="id_tour_detail_temp" name="Tour Detail">

            <t t-call="website.layout">
                <t t-foreach="res" t-as="v">

                    <div class="container" id="tour_detail">
                        <div classs="col-lg-12" style="margin-bottom:10px;">
                            <div class="col-sm-4 oe_product_image">
                                <!--<span t-field="v.image_medium" t-field-options='{"widget": "image", "class": "img-rounded"}'/>-->
                                <img t-att-src="'data:image/png;base64,%s' % v.image_medium" class="img-rounded img-responsive" width="100%" height="100%"/>
                            </div>
                            <div class="col-sm-8">
                                <div class="table-responsive">
                                    <table class="table">
                                        <tr>
                                            <td><h3><strong>Name of Tour</strong></h3></td>
                                            <td><h3><span t-field="v.name"/></h3></td>
                                        </tr>
                                        <tr>
                                            <td><strong>Tour Type</strong></td>
                                            <td><span t-field="v.tour_type"/></td>
                                        </tr>
                                        <tr>
                                            <td><strong>Tour Code</strong></td>
                                            <td><span t-field="v.tour_code"/></td>
                                        </tr>
                                        <tr>
                                            <td><strong>No of Nights</strong></td>
                                            <td><span t-field="v.no_of_nights"/></td>
                                        </tr>
                                        <tr>
                                            <td><strong>Tour Date</strong></td>
                                            <td><span t-field="v.start_date"/></td>
                                        </tr>
                                    </table>
                                </div>
                                <div class="col-md-12">
                                    <div class="col-md-4"><a t-att-href="keep('/tour_login',tour_id = v.id, name = v.name)"><button type="submit" class="btn btn-primary">Book Tour</button></a></div>
                                    <div class="col-md-4"><a t-att-href="keep('/tour_login',tour_id = v.id, name = v.name)"><button type="submit" class="btn btn-primary">Cancel Tour</button></a></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="col-lg-4">
                                <strong><h2>About This Tour</h2></strong><br/>
                                <p><span t-field="v.note"/></p>
                            </div>
                            <div class="col-lg-8">
                                <h2><span class="title">Iternary</span></h2>
                                <div class="table-responsive" id="table_itenary">
                                    <table class="table table-condensed">
                                        <thead>
                                            <tr>
                                                <th>Day</th>
                                                <th>Iteranry</th>
                                                <th>Night Hold</th>
                                                <th>Hotel</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <t t-foreach="v.itenary" t-as="itrny">
                                                <tr>
                                                    <td><span t-field="itrny.day"/></td>
                                                    <td><span t-field="itrny.itenary"/></td>
                                                    <td><span t-field="itrny.night_hold"/></td>
                                                    <td><t t-foreach="itrny.hotel_detail" t-as="hotel"><span t-field="hotel.name"/></t></td>
                                                </tr>
                                            </t>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </template>

        <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
        <!--Search template-->
        <template id="tour_website_search" name="Search">
            <form t-att-action="keep2('/tour', search=0)" t-att-class="search_class">

                <t t-if="attrib_values">
                    <t t-foreach="attrib_values" t-as="a">
                        <input type="hidden" name="attrib" t-att-value="'%s-%s' % (a[0], a[1])"/>
                    </t>
                </t>

                <div class="input-group">
                    <input type="text" name="search" class="search-query form-control" placeholder="Search..." t-att-value="search"/>
                    <span class="input-group-btn">
                        <a class="btn btn-default a-submit"><i class="fa fa-search"/></a>
                    </span>
                </div>
            </form>
        </template>

        <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
        <!--Login Template-->
        <template id="id_tour_login_temp" name="Tour Login Detail">

            <t t-call="website.layout">

                <div class="container">
                    <div id="login_form">
                        <form class="form-horizontal" role="form" t-att-action="keep('/book_tour')">

                            <input type='text' name='tour_id'  t-att-value="tour_id.id" style="display: none;"/>

                            <div class="form-group">
                                <label class="control-label"><h2>Login</h2></label>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-sm-4" for="email">Email:</label>
                                <div class="col-sm-4">
                                    <input type="email" class="form-control" id="email" name="email" placeholder="Enter email" required="true"/>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-sm-4" for="pwd">Password:</label>
                                <div class="col-sm-4">
                                    <input type="password" class="form-control" id="pwd" name="pwd" placeholder="Enter password" required="true"/>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-2">
                                    <!--<div class="checkbox">-->
                                    <label><input type="checkbox"/> Remember me</label>
                                    <!--</div>-->
                                </div>
                                <div class="col-sm-2">
                                    <a t-att-href="keep('/forgot_password',tour_id = tour_id.id)">Forgot  Password?</a>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-2">
                                    <button type="submit" class="btn btn-success">Login</button>
                                </div>
                                <div class="col-sm-2">
                                    <button type="reset" class="btn btn-danger">Reset</button>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-sm-offset-4 col-sm-2">
                                    <a t-att-href="keep('/tour_registration',tour_id = tour_id.id)"><button type="button" class="btn btn-primary">Register</button></a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <!--</div>-->
            </t>
        </template>

        <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
        <!--Registration Template-->
        <template id="id_tour_registration_temp" name="Tour Registration Detail">

            <t t-call="website.layout">

                <div class="container">

                    <form class="form-horizontal" name="registration" role="form" t-att-action="keep('/tour_registration_create', tour_id=tour_id.id)">

                        <input type='text' name='tour_id'  t-att-value="tour_id.id" style="display: none;"/>

                        <div class="form-group">
                            <label class="control-label col-sm-4"><h2>Registration</h2></label>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Name:</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" name='name' id="name" placeholder="Enter Full Name" required="true"/>
                                <div id="name_msg"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4" >Gender:</label>
                            <div class="col-sm-4">
                                <label class="col-sm-4"><input type="radio" name="gender" value="Male"/>  Male</label>
                                <label class="col-sm-4"><input type="radio" name="gender" value="Female"/>  Female</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Phone:</label>
                            <div class="col-sm-4">
                                <input class="form-control" id="phone" placeholder="Enter Phone number" name='phone' type="number" required="true"/>
                            </div>
                            <div class="col-sm-4" id="phone_msg"></div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Email:</label>
                            <div class="col-sm-4">
                                <input type="email" class="form-control" id="email" placeholder="Enter email" name='email' required="true" />
                                <div id="email_msg"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">User Id:</label>
                            <div class="col-sm-4">
                                <input type="text" class="form-control" id="uid" placeholder="Enter User Id" name='user_id'/>
                                <div id="uid_msg"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Password:</label>
                            <div class="col-sm-4">
                                <input type="password" class="form-control" id="pass" placeholder="Enter password" name='password' required="true"/>
                                <div id="pass_msg"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-4">Confirm Password:</label>
                            <div class="col-sm-4">
                                <input type="password" class="form-control" id="cpass" placeholder="Enter Confirm password" name='cpassword' required="true"/>
                                <div id="cpass_msg"></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-4 col-sm-4">
                                <div class="checkbox">
                                    <label><input type="checkbox"/> Remember me</label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-4 col-sm-2">
                                <button type="submit" class="btn btn-primary" onClick="myFunction()">Register</button>
                            </div>
                            <div class="col-sm-2">
                                <button type="reset" class="btn btn-danger">Reset</button>
                            </div>
                        </div>

                    </form>
                </div>
            </t>
        </template>


        <!--/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
